{
  "name": "@opencage/leaflet-opencage-geocoding",
  "version": "3.0.0-alpha.2",
  "type": "module",
  "description": "Leaflet plugin for geocoding via the OpenCage Geocoder API",
  "repository": {
    "type": "git",
    "url": "git@github.com:opencagedata/leaflet-opencage-geocoding.git"
  },
  "homepage": "https://opencagedata.com",
  "author": {
    "name": "OpenCage GmbH",
    "email": "support@opencagedata.com",
    "url": "https://opencagedata.com"
  },
  "license": "BSD",
  "keywords": [
    "leaflet",
    "plugin",
    "geocoding",
    "search",
    "map",
    "opencagedata"
  ],
  "scripts": {
    "build": "npm run clean && vite build && vite build --mode development",
    "dev": "vite",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "clean": "rm -rf dist"
  },
  "main": "dist/js/OpenCageGeocoding.min.js",
  "module": "dist/js/OpenCageGeocoding.esm.js",
  "browser": "dist/js/OpenCageGeocoding.min.js",
  "unpkg": "dist/js/OpenCageGeocoding.min.js",
  "exports": {
    ".": {
      "import": "./dist/js/OpenCageGeocoding.esm.js",
      "require": "./dist/js/OpenCageGeocoding.min.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "leaflet": "2.0.0-alpha.1",
    "opencage-api-client": "2.1.0"
  },
  "peerDependencies": {
    "leaflet": "^2",
    "opencage-api-client": "^2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "eslint": "9.39.2",
    "globals": "17.3.0",
    "jsdom": "28.0.0",
    "prettier": "3.8.1",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  }
}
